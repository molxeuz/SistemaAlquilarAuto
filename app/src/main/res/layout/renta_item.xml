<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        app:cardCornerRadius="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:elevation="2dp">

        <LinearLayout
            android:layout_width="410dp"
            android:layout_height="270dp">

            <LinearLayout
                android:layout_width="370dp"
                android:layout_height="250dp"
                android:orientation="vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp">

                <TextView
                    android:layout_width="120dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textColor="#000000"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:text="NUMERO RENTA:"/>

                <TextView
                    android:id="@+id/tvnumero_lista"
                    android:layout_width="80dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="145dp"
                    android:layout_marginTop="-20dp"
                    android:textSize="15dp"/>

                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:background="#C3C3C3">

                    <LinearLayout
                        android:layout_width="280dp"
                        android:layout_height="175dp"
                        android:background="#E3E3E3"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="10dp">

                        <LinearLayout
                            android:layout_width="110dp"
                            android:layout_height="35dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:background="#9261EA">

                            <LinearLayout
                                android:layout_width="100dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#FFFFFF">

                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="25dp"
                                    android:textAlignment="center"
                                    android:backgroundTint="#FFFFFF"
                                    android:text="PLACA AUTO"
                                    android:textColor="@color/black"
                                    android:textSize="13dp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </LinearLayout>

                        <TextView
                            android:id="@+id/tvusuario_lista"
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="12dp"
                            android:textSize="20dp"/>

                        <LinearLayout
                            android:layout_width="110dp"
                            android:layout_height="35dp"
                            android:layout_marginLeft="-225dp"
                            android:layout_marginTop="50dp"
                            android:background="#9261EA">

                            <LinearLayout
                                android:layout_width="100dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#FFFFFF">

                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="25dp"
                                    android:textAlignment="center"
                                    android:backgroundTint="#FFFFFF"
                                    android:text="CLIENTE"
                                    android:textColor="@color/black"
                                    android:textSize="13dp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </LinearLayout>

                        <TextView
                            android:id="@+id/tvplaca_lista"
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="55dp"
                            android:textSize="20dp"/>

                        <LinearLayout
                            android:layout_width="110dp"
                            android:layout_height="35dp"
                            android:layout_marginLeft="-225dp"
                            android:layout_marginTop="90dp"
                            android:background="#9261EA">

                            <LinearLayout
                                android:layout_width="100dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#FFFFFF">

                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="25dp"
                                    android:textAlignment="center"
                                    android:backgroundTint="#FFFFFF"
                                    android:text="FECHA RENTA"
                                    android:textColor="@color/black"
                                    android:textSize="13dp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </LinearLayout>

                        <TextView
                            android:id="@+id/tvfecha_inicial_lista"
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="14dp"
                            android:layout_marginTop="94dp"
                            android:textSize="20dp"/>

                        <LinearLayout
                            android:layout_width="110dp"
                            android:layout_height="35dp"
                            android:layout_marginLeft="-225dp"
                            android:layout_marginTop="130dp"
                            android:background="#9261EA">

                            <LinearLayout
                                android:layout_width="100dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="5dp"
                                android:background="#FFFFFF">

                                <TextView
                                    android:layout_width="100dp"
                                    android:layout_height="20dp"
                                    android:layout_marginTop="2dp"
                                    android:textAlignment="center"
                                    android:backgroundTint="#FFFFFF"
                                    android:text="FECHA ENTREGA"
                                    android:textColor="@color/black"
                                    android:textSize="11dp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                        </LinearLayout>

                        <TextView
                            android:id="@+id/tvfecha_final_lista"
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="14dp"
                            android:layout_marginTop="135dp"
                            android:textSize="20dp"/>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>

</LinearLayout>